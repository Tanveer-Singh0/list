		// header background change when scroll
        document.addEventListener("scroll", (event) => {
            if (window.scrollY > 0) {
                $("header").css('background-color', '#fff');
            } else {
                $("header").css('background-color', 'transparent');
            }
        })

        $(document).ready(function () {
            if (window.scrollY > 0) {
                $("header").css('background-color', '#fff');
            } else {
                $("header").css('background-color', 'transparent');
            }
        })

		// class slider
		$("#newsSection").multislider({
			interval: 0,
			slideAll: false,
			duration: 500,
		});

